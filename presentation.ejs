console.log(io);
if (window.io) {
var socket = io.connect('http://<%= address %>:3000');

  socket.on('next', function (stepId) {
    <%= nextFunction %>;
  });


  socket.on('prev', function (stepId) {
    <%= prevFunction %>;
  });

  document.onkeydown = function(e) {
    var key;
    if (window.event) {
      key = window.event.keyCode;
    } else {
      key = e.keyCode;
    }
    if (key == 39) {
      socket.emit('next');
    } else if (key == 37) {
      socket.emit('prev');
    }
  };
}
