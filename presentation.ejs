if (window.io) {
var socket = io.connect('http://<%= address %>:3000');

  socket.on('next', function (pos) {
    console.log(pos);
    <%= nextFunction %>;
  });


  socket.on('prev', function (pos) {
    <%= prevFunction %>;
  });

  document.onkeydown = function(e) {
    var key;
    if (window.event) {
      key = window.event.keyCode;
    } else {
      key = e.keyCode;
    }

    switch (key) {
      case 32:
        // Space
        socket.emit('next', <%= nextCall %>);
        break;        
      case 37:
        // Left arrow
        socket.emit('prev', <%= prevCall %>);
        break;
      case 39:
        // Right arrow
        socket.emit('next', <%= nextCall %>);
        break;
      default:
        break;
    }
  };
}
